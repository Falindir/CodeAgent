<div class="row">
  <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad" >
    <div class="panel panel-info">
      <div class="panel-heading">
        <h3 class="panel-title">Profil : {{user.name}}</h3>
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-md-3 col-lg-3 " align="center"> <img alt="User Pic" src="data:image/jpeg;base64," class="img-circle img-responsive"> </div>
      <div class=" col-md-9 col-lg-9 ">
        <table class="table table-user-information">
          <tbody>
            <tr>
              <td>Name:</td>
              <td>{{user.name}}</td>
            </tr>
            <tr>
              <td>Inscription:</td>
              <td>{{user.inscriptionDate}}</td>
            </tr>
            <tr>
              <td>Premium:</td>
              <td>{{user.premium}}</td>
            </tr>
            <tr>
              <td>Role:</td>
              <td>{{user.role}}</td>
            </tr>
            <tr>
              <td>Rank:</td>
              <td>{{user.rank}}</td>
            </tr>
          </tbody>
        </table>

      </div>
    </div>
  </div>
  <div class="panel-footer">
    <a data-original-title="Broadcast Message" data-toggle="tooltip" type="button" class="btn btn-sm btn-primary">Edit Profil</a>
  </div>

</div>
</div>

</div>
